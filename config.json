{
	"name": "Project EM : A.I. Based Medical Image Diagnosis ",

	"namespace": "Stanford\\AIMI",

	"description": "Stanford ML-models as downloadable, plug-and-play modules for all REDCap users",

	"documentation": "README.md",

	"authors": [
		{
			"name": "Andrew Martin",
			"email": "andy123@stanford.edu",
			"institution": "Stanford University"
		},
		{
			"name": "Irvin Szeto",
			"email": "irvins@stanford.edu",
			"institution": "Stanford University"
		},
		{
			"name": "Jordan Schultz",
			"email": "jmschultz@stanford.edu",
			"institution": "Stanford University"
		}
	],

	"framework-version": 5,

	"permissions": [
		"redcap_module_system_enable",
		"redcap_module_project_enable",
		"redcap_module_save_configuration"
	],
	
	"enable-every-page-hooks-on-system-pages": false,	

	"links": {
		"project": [
			{
		        "name": "Run Model",
		        "icon": "gear",
		        "url" : "pages/aimi.php",
		        "show-header-and-footer": false
			},
			{
		        "name": "Select/Config Model",
		        "icon": "gear",
		        "url" : "pages/config_model.php",
		        "show-header-and-footer": false
			}
		],
		"control-center": [

		]
	},

	"no-auth-pages": [
		"endpoints/syncData"
	],

	"project-settings": [
		{
			"key": "enable-project-debug-logging",
			"name": "<b>Enable Debug Logging</b><br><i>(optional)</i> Requires installation and configuration of emLogger",
			"required": false,
			"type": "checkbox"
		}
	],

	"crons": [
		{
			"cron_name": "aimi_cron",
			"cron_description": "Cron that runs once a day",
			"method": "projectCron",
			"cron_frequency": "86400",
			"cron_max_run_time": "300"
		}
	],

	"system-settings": [
		{
			"key": "enable-system-debug-logging",
			"name": "<b>Enable Debug Logging (system-wide)</b><i>(optional)</i> Requires installation and configuration of emLogger",
			"required": false,
			"type": "checkbox"
		}
	],

	"compatibility": {
		"php-version-min": "",
		"php-version-max": "",
		"redcap-version-min": "",
		"redcap-version-max": ""
	}
}
